# Marketing Cloud JWT Signing Secret
# Get this from your Marketing Cloud Installed Package
MC_JWT_SIGNING_SECRET=your_jwt_signing_secret_here

# Iron Session Password (at least 32 characters)
# Generate a secure random string for production
SESSION_PASSWORD=complex_password_at_least_32_characters_long

# Session Cookie Name
SESSION_COOKIE_NAME=mc_app_session

# App URL (used for redirects)
# In production, set this to your Vercel URL
NEXT_PUBLIC_APP_URL=http://localhost:3000
